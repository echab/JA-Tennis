  "scripts": {
    "start": "vite",
    "test": "jest",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "old2_scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:ts && npm run bundle:esm && npm run bundle:esm:min && npm run bundle:umd",
    "build:ts": "npm run lint && tsc --project tsconfig.build.json",
    "test": "jest",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "bundle:esm": "rollup dist/index.js --file dist/index.mjs --format esm",
    "bundle:esm:min": "terser --ecma 6 --compress --mangle --module -o dist/index.min.mjs -- dist/index.mjs && gzip -9 -c dist/index.min.mjs > dist/index.min.mjs.gz",
    "bundle:umd": "rollup dist/index.js --file dist/index.umd.js --format umd --name sayHello"
  },
  "old_scripts": {
    "bundle:umd:min": "terser --ecma 6 --compress --mangle -o dist/index.umd.min.js -- dist/index.umd.js && gzip -9 -c dist/index.umd.min.js > dist/index.umd.min.js.gz",
    "build:stats": "(echo '\\033[35;3m' ; cd dist && ls -lh index*js index*gz | tail -n +2 | awk '{print $5,$9}')"
  },
  "_main": "dist/index.js",
  "_module": "dist/index.min.mjs",
  "_unpkg": "dist/index.umd.min.js",
  "_types": "dist/index.d.ts",
  "_files": [
    "dist"
  ],
  "dependencies": {
    "@solidjs/router": "^0.5.0",
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.2.1",
    "postcss": "^8.4.18",
    "solid-devtools": "^0.18.0",
    "solid-jest": "^0.2.0",
    "solid-testing-library": "^0.3.0",
    "tailwindcss": "^3.1.8",
    "terser": "^5.15.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  }
}
